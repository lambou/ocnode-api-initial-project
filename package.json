{
  "name": "api-starter-typescript",
  "version": "1.0.0",
  "description": "Norea.js starter typescript project",
  "repository": "https://github.com/noreajs/api-starter-typescript.git",
  "main": "index.js",
  "scripts": {
    "build-ts": "tsc",
    "start": "node dist/index.js",
    "start:local": "env-cmd -f ./.env.local nodemon ./src/index.ts",
    "start:development": "env-cmd -f ./.env.development nodemon ./src/index.ts",
    "start:production": "env-cmd -f ./.env.production nodemon ./src/index.ts",
    "prod": "npm run build && npm run start",
    "postinstall": "npm run build-ts"
  },
  "author": "OvniCode",
  "license": "ISC",
  "dependencies": {
    "@noreajs/common": "^1.0.1-0",
    "@noreajs/core": "^1.1.2-0",
    "@noreajs/mongoose": "0.0.18",
    "@noreajs/oauth-v2-provider-mongoose": "0.0.1-5",
    "axios": "^0.19.2",
    "env-cmd": "^10.0.1",
    "express-jwt": "^5.3.3",
    "express-useragent": "^1.0.13",
    "mailgen": "^2.0.10",
    "moment": "^2.24.0",
    "nodemailer": "^6.4.10",
    "serialize-error": "^5.0.0",
    "socket.io": "^2.3.0",
    "underscore": "^1.9.2",
    "unixcrypt": "^1.0.10",
    "validate.js": "^0.13.1",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "@types/express-useragent": "^1.0.0",
    "@types/mailgen": "^2.0.3",
    "@types/node": "^13.1.2",
    "@types/nodemailer": "^6.4.0",
    "@types/socket.io": "^2.1.4",
    "@types/underscore": "^1.10.0",
    "@types/validator": "^12.0.1",
    "nodemon": "^2.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  }
}
